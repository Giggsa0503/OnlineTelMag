@model OnlineTelMag.Data.Order

@{
    ViewData["Title"] = "Details";
}

<h1>Детайли</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Потребител
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Users.FullName)
        </dd>
        <dt class = "col-sm-2">
            Продукт
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Telephones.TelName)
        </dd>
        <dt class = "col-sm-2">
            Дата
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DateRegister)
        </dd>
    </dl>
</div>
<div>
    <a style="color:lime" asp-action="Edit" asp-route-id="@Model.Id">Пормени</a> |
    <a style="color:lime" asp-action="Index">Назад</a>
</div>
