@model OnlineTelMag.Data.Telephone

@{
    ViewData["Title"] = "Delete";
}

<h1>Изтриване</h1>

<h3>Искате ли да изтриете този продукт ?</h3>
<div>
   
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Име
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TelName)
        </dd>
        <dt class = "col-sm-2">
           Модел
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Brands.NameModel)
        </dd class>
        <dt class = "col-sm-2">
            Цена
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Prise)
        </dd>
        <dt class = "col-sm-2">
            Описание
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Description)
        </dd>
        <dt class = "col-sm-2">
            Цвят
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Color)
        </dd>
        <dt class = "col-sm-2">
           Брой
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Broi)
        </dd>
        <dt class = "col-sm-2">
           Дата
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Date)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Изтрий" class="btn btn-danger" /> |
        <a style="color:lime" asp-action="Index">Назад</a>
    </form>
</div>
